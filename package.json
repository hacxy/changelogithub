{
  "name": "@hacxy/changelogithub",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.11.0",
  "description": "Generate changelog for GitHub.",
  "author": "hacxy",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/changelogithub#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/changelogithub.git"
  },
  "bugs": "https://github.com/antfu/changelogithub/issues",
  "keywords": [
    "github",
    "release",
    "releases",
    "conventional",
    "changelog",
    "log"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./cli.mjs",
  "files": [
    "*.mjs",
    "dist"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "test": "vitest",
    "lint": "eslint .",
    "release": "bumpp --commit --push --tag",
    "start": "nr dev && node cli.mjs",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.10",
    "c12": "^1.11.2",
    "cac": "^6.7.14",
    "changelogen": "0.5.5",
    "convert-gitmoji": "^0.1.5",
    "execa": "^9.4.0",
    "kolorist": "^1.8.0",
    "ofetch": "^1.4.0",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/minimist": "^1.2.5",
    "@types/semver": "^7.5.8",
    "bumpp": "^9.6.1",
    "eslint": "^9.12.0",
    "esno": "^4.8.0",
    "fs-extra": "^11.2.0",
    "typescript": "^5.6.2",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.2"
  }
}
